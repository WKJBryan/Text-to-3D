# Model Configuration
DEFAULT_MODEL=llama3.1:8b
PERFORMANCE_MODE=balanced

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_TIMEOUT=300

# Export Configuration
EXPORT_DIRECTORY=./exports

# UI Configuration
WINDOW_SIZE=900x700
THEME=dark

# Generation Settings
VOXEL_RESOLUTION=0.5
MAX_GENERATION_TIME=300
MAX_FILE_SIZE_MB=50

# RAG Configuration
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_CACHE_DIR=./embeddings
VECTOR_SEARCH_TOP_K=2
SIMILARITY_THRESHOLD=0.3

# ======================================
# üñ®Ô∏è 3D PRINTING INTEGRATION SETTINGS
# ======================================

# PrusaSlicer Configuration
PRUSASLICER_PATH=C:\Program Files\Prusa3D\PrusaSlicer\prusa-slicer.exe
SLICER_CONFIG_DIR=./slicer_configs
TEMP_GCODE_DIR=./temp_gcode

# Mainsail/Klipper Configuration  
MAINSAIL_URL=http://192.168.1.100
MAINSAIL_TIMEOUT=30
PRINTER_NAME=Voron_2.4_350

# Voron 2.4 350 Printer Specifications
PRINTER_BED_X=350
PRINTER_BED_Y=350
PRINTER_BED_Z=350
NOZZLE_DIAMETER=0.4
FILAMENT_DIAMETER=1.75

# Default Slicing Settings (Standard Profile)
LAYER_HEIGHT=0.2
INFILL_PERCENTAGE=20
PRINT_SPEED=60
TRAVEL_SPEED=150
SUPPORT_MATERIAL=auto
BRIM_WIDTH=5

# PLA Material Settings
EXTRUDER_TEMP=210
BED_TEMP=60
CHAMBER_TEMP=0

# Advanced Voron Settings
PRESSURE_ADVANCE=0.04
MAX_ACCEL=3000
MAX_VELOCITY=300
RETRACTION_LENGTH=0.5
RETRACTION_SPEED=40

# Print Management
AUTO_SLICE_AFTER_EXPORT=false
AUTO_START_PRINT_AFTER_SLICE=false
MAX_GCODE_FILE_SIZE_MB=100
PRINT_QUEUE_MAX_SIZE=5

# Safety Settings
CHECK_PRINTER_STATUS_BEFORE_PRINT=true
CONFIRM_BEFORE_PRINT=true
HEATED_BED_SAFETY_CHECK=true
